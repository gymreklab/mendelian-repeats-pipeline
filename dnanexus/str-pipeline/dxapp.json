{
  "name": "str-pipeline",
  "title": "str pipeline",
  "summary": "This is to test the str pipeline",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "inputSpec": [
    {
      "name": "ConfigFile",
      "label": "Configuration File",
      "class": "string",
      "optional": false,
      "help": ""
    },
    {
      "name": "bams",
      "label": "bam files used in analysis",
      "class": "array:file",
      "optional": false,
      "patterns": [
        "*"
      ],
      "help": ""
    }
  ],
  "outputSpec": [
    {
      "name": "vcfs",
      "label": "vcf output files",
      "class": "array:file",
      "patterns": [
        "*"
      ],
      "help": ""
    }
  ],
  "runSpec": {
    "timeoutPolicy": {
      "*": {
        "hours": 48
      }
    },
    "interpreter": "bash",
    "release": "14.04",
    "distribution": "Ubuntu",
    "file": "src/str-pipeline.sh"
  },
  "access": {
    "project": "CONTRIBUTE",
    "network": [
      "*"
    ]
  },
  "regionalOptions": {
    "aws:us-east-1": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem1_ssd1_x4"
        }
      }
    }
  }
}
